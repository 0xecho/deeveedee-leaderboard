datasource db {
  provider = "postgresql"
  url      = env("DB_URL")
}

generator client {
  provider = "prisma-client-js"
}

model Leaderboard {
  createdAt    DateTime @default(now())
  id           String   @id @default(cuid())
  max_credits  String?
  name         String?
  timeSurvived String?
  updatedAt    DateTime @updatedAt
}
